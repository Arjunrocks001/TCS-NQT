
#include <iostream>
using namespace std;

int main()
{
    string s;
    
    getline(cin,s);
    
    int cntstar = 0;
    int cnthash = 0;
    
    for(int i=0;i<s.size();i++)
    {
        if(s[i]=='*')
        {
            cntstar++;
            
        }
        else{
            cnthash++;
        }
    }
    cout<<cntstar - cnthash;
    return 0;
}
